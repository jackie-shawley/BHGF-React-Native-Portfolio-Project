{
  "expo": {
    "name": "bhgf-react-native",
    "slug": "bhgf-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/square-ruffle-cake.png",    
    "description": "A React Native app built for a fictional bakery and cafe",
    "primaryColor": "#1af5c3",
    "splash": {
      "image": "./assets/images/square-ruffle-cake.png",
      "resizeMode": "contain",
      "backgroundColor": "#1af5c3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/square-ruffle-cake.png",
        "backgroundColor": "#1af5c3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
